{
  "name": "telegram",
  "version": "1.0.0",
  "description": "ws",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:rollback": "npx sequelize-cli db:migrate:undo",
    "migrate:rollback:all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:rollback": "npx sequelize-cli db:seed:undo",
    "seed:rollback:all": "npx sequelize-cli db:seed:undo:all",
    "seed:undo": "npx sequelize-cli db:seed:undo --seed ",
    "db:reset": "npm run seed:rollback:all && npm run migrate:rollback:all && npm run migrate",
    "db:refresh": "npm run db:reset && npm run seed:all"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "10.0.3",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "10.0.3",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/platform-express": "10.0.3",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/terminus": "^10.1.1",
    "@telegraf/session": "^2.0.0-beta.6",
    "axios": "^1.6.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "helmet": "^7.0.0",
    "kysely": "^0.23.5",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "nestjs-telegraf": "^2.7.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.10",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "sequelize": "^6.34.0",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.5",
    "telegraf": "^4.15.0",
    "telegram": "^2.19.10",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.0.5",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.3",
    "@types/cron": "^2.4.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.3",
    "@types/pg": "^8.10.7",
    "@types/sequelize": "^4.28.17",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "eslint": "8.42.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typegram": "^5.2.0",
    "typescript": "5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
